{
  "name": "@editvr/aframe-dialog-popup-component",
  "version": "0.0.0-development",
  "description": "Provides a component that constructs a toggle-able dialog with a title, description, image, and close button.",
  "main": "dist/aframe-dialog-popup-component.js",
  "unpkg": "dist/aframe-dialog-popup-component.min.js",
  "scripts": {
    "build": "webpack index.js dist/aframe-dialog-popup-component.js",
    "dev": "budo index.js:dist/aframe-dialog-popup-component.min.js --port 7000 --live --open",
    "dist": "npm run build && uglifyjs dist/aframe-dialog-popup-component.js > dist/aframe-dialog-popup-component.min.js",
    "lint": "eslint --ext .jsx,.js .",
    "prepublishOnly": "npm run dist",
    "prettier": "prettier --write \"**/*.{js,jsx,json,css}\"",
    "precommit": "lint-staged -c lint-staged.config.js ; yarn lint ; yarn test:ci",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "ghpages": "ghpages",
    "start": "npm run dev",
    "semantic-release": "semantic-release",
    "test": "karma start ./tests/karma.conf.js",
    "test:ci": "karma start ./tests/karma.conf.js --single-run",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/editvr/aframe-dialog-popup-component.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "webvr",
    "dialog-popup"
  ],
  "author": "Patrick Coffey <patrickcoffey48@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/editvr/aframe-dialog-popup-component/issues"
  },
  "homepage": "https://github.com/editvr/aframe-dialog-popup-component#readme",
  "release": {
    "verifyConditions": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@commitlint/cli": "^7.0.0",
    "@semantic-release/github": "^4.2.18",
    "@semantic-release/npm": "^3.3.4",
    "aframe": "*",
    "babel-loader": "^8.0.0-beta",
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "chai": "^3.4.1",
    "chai-shallow-deep-equal": "^1.3.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "ghpages": "^0.0.8",
    "husky": "^0.14.3",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "lint-staged": "^7.2.0",
    "mocha": "^2.3.4",
    "prettier": "^1.13.7",
    "randomcolor": "^0.4.4",
    "semantic-release": "^15.6.0",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "snazzy": "^4.0.0",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "webpack": "^2.7.0"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  },
  "dependencies": {
    "@commitlint/config-angular": "^7.0.1"
  }
}
